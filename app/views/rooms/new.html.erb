<div class='new-room-page'>
  <div class='new-room'>
    <p class='page-name'>新規ルーム</p>
    <div class='page-form'>
      <%= form_with model: @room, url: '#' do |f| %>
        <div class='form-group'>
          <div class='form-text-rap'>
            <label class='form'>ルーム名</label>
            <span class="indispensable">(必須)</span>
          </div>
          <%= f.text_field :name %>
        </div>
        <div class='form-group'>
          <div class='form-text-rap'>
            <label class='form'>参加者リスト</label>
            <span class="indispensable">(必須)</span>
          </div>
          <%= f.select :list %>
        </div>
      <% end %>
    </div>
  </div>
  
</div>